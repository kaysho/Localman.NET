<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>netstandard2.0</TargetFramework>
	<PackageId>Localman.NET</PackageId>
	<Version>1.0.0</Version>
	<Authors>Damilola Shopeyin</Authors>
	  <PackageTags>State nigeria lga capital</PackageTags>
	  <PackageProjectUrl>https://github.com/kaysho/Localman.NET</PackageProjectUrl>
	  <RepositoryType>git</RepositoryType>
	  <IncludeSymbols>true</IncludeSymbols>
	  <Description>A .NET library that provides Nigerian based location data list and info such as States and Local Government Areas</Description>
	<IsPackable>true</IsPackable>
  </PropertyGroup>
	
  <ItemGroup>
	<EmbeddedResource Include="state_details.json" />
  </ItemGroup>

  <ItemGroup>
	<PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
